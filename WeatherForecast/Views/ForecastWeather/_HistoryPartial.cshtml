@using DevExtreme.AspNet.Mvc;

@model WeatherForecast.Models.HistoricalDataModel

<div>
    <div>
        @Html.Hidden("MasterKey", Model.MasterKey)
        @Html.Hidden("DetailKey", Model.DetailKey)
    </div>

    <div style="padding-left:10px;padding-right:10px;">
      
        <div class="row">
            @Html.Partial("_WeatherForecastDetails", Model.weatherForecastLiveDataModel)
        </div>
    </div>
    <div class="col-lg-12" style="padding:15px !important;  border: 1px solid #ddd;">
        <div class="col-lg-4" style="padding:0px !important">
            <h1>Master</h1>

            @Html.Partial("_MasterGrid", Model)

        </div>
        <div class="col-lg-8" style="padding: 0px !important; border-left: solid #ddd; ">
            <h1>Details</h1>
            @Html.Partial("_DetailsGrid", Model)
        </div>

    </div>
</div>
